<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#71bdB2">

    <TextView
        android:id="@+id/datos"
        android:layout_width="104dp"
        android:layout_height="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:text="Salir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="logout"
        android:textColor="@color/com_facebook_button_text_color"
        android:background="@color/com_facebook_button_background_color"/>

    <Button
        android:id="@+id/aplicarTest"
        android:layout_width="268dp"
        android:layout_height="81dp"
        android:layout_marginTop="256dp"
        android:background="#279989"
        android:text="Aplicar Test"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="aplicarTest"/>

    <Button
        android:id="@+id/verMisAudios"
        android:layout_width="268dp"
        android:layout_height="81dp"
        android:layout_marginTop="428dp"
        android:background="#279989"
        android:text="Ver mis audios"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="verMisAudios"/>
</androidx.constraintlayout.widget.ConstraintLayout>